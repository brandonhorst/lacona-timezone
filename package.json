{
  "name": "lacona-timezone",
  "version": "1.0.0",
  "description": "Lacona Addon for doing time zone math",
  "main": "lib/index.js",
  "lacona": {
    "title": "Timezone",
    "description": "Convert between time zones",
    "config": "config.json"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "lacona",
    "lacona-addon"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona-timezone.git"
  },
  "dependencies": {
    "elliptical": "^1.2.0",
    "elliptical-datetime": "^1.0.1",
    "lacona-api": "^1.4.0",
    "lacona-phrases": "^1.0.1",
    "lodash": "^4.15.0",
    "moment": "^2.15.0",
    "timezone-names": "0.0.3"
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015-node": "^6.1.1",
    "rimraf": "^2.5.4"
  },
  "babel": {
    "presets": [
      "es2015-node"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
